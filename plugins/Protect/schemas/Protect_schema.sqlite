create table mt_protect (
    protect_id integer not null primary key,
    protect_entry_id integer not null,
    protect_blog_id integer not null,
    protect_password varchar(200),
    protect_type varchar(10) not null,
    protect_data bytea,
);
create index protect_entry_id on mt_protect (protect_entry_id);
create index protect_blog_id on mt_protect (protect_blog_id);

create table mt_protect_groups (
	protect_groups_id integer not null primary key,
	protect_groups_label varchar(100) not null,
	protect_groups_description varchar(255),
	protect_groups_type varchar(10) not null,
	protect_groups_data bytea,
	unique(protect_groups_label)
); 
create index protect_groups_label on mt_protect_groups (protect_groups_label);